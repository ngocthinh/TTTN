<% provide :title, t(".add_category") %>
<h1><%= t ".add_category" %></h1>
<div class="row">
  <div class="col-md-6 col-md-offset-3">
   <%= form_for @category, url: admin_categories_path do |f| %>
      <%= f.label :name %>
      <%= f.text_field :name, class: 'form-control' %>
      <%= f.label :decription %>
      <%= f.text_area :decription, class: 'form-control' %>
      <%= f.submit t(".add"), class: "btn btn-primary" %>
    <% end %>
  </div>
</div>
